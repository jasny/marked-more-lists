{
  "name": "marked-more-lists",
  "version": "1.0.0",
  "description": "An extension for marked to support additional list styles",
  "main": "./lib/index.cjs",
  "module": "./src/index.js",
  "browser": "./lib/index.umd.js",
  "types": "./src/index.d.ts",
  "type": "module",
  "keywords": [
    "marked",
    "extension"
  ],
  "files": [
    "lib/",
    "src/"
  ],
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "format": "eslint --fix",
    "lint": "eslint",
    "test": "jest --verbose",
    "test:cover": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasny/marked-more-lists.git"
  },
  "author": "Arnold Daniels <arnold@jasny.net> (https://jasny.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jasny/marked-more-lists/issues"
  },
  "homepage": "https://github.com/jasny/marked-more-lists#readme",
  "peerDependencies": {
    "marked": ">=14 <15"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@markedjs/eslint-config": "^1.0.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "babel-jest": "^29.7.0",
    "eslint": "^9.14.0",
    "globals": "^15.11.0",
    "jest-cli": "^29.7.0",
    "marked": "^15.0.2",
    "rollup": "^4.24.3",
    "semantic-release": "^24.2.0"
  }
}
